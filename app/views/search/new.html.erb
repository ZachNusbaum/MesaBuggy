<h1>Search#new</h1>
<%= search_form_for @q, url: results_path, method: :get do |f| %>
	<div class="form-group">
		<%= f.label :name_or_description_cont %>
		<%= f.search_field :name_or_description_cont, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :category_name_eq %>
		<%= f.search_field :category_name_eq, class: 'form-control' %>
	</div>

	<%= f.submit class: 'btn btn-primary' %>
<% end %>
